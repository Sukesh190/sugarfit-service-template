root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

[*.java]
indent_size = 4

[*.{xml,yml,yaml}]
indent_size = 2

[*.properties]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[{mvnw,mvnw.cmd}]
end_of_line = unset
